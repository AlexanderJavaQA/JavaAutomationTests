{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "statusName": { "type": "string" },
    "erknmId": { "type": "string" },
    "knoOrganization": { "type": "string" },
    "kindControl": { "type": "string" },
    "kind": { "type": "string" },
    "startDate": { "type": "string", "format": "date" },
    "stopDate": { "type": "string", "format": "date" },
    "announcementDate": { "type": "string", "format": "date" },
    "ogrn": { "type": "string" },
    "inn": { "type": "string" },
    "organizationName": { "type": "string" },
    "objects": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "address": { "type": "string" },
          "objectType": { "type": "string" },
          "objectKind": { "type": "string" },
          "objectSubKind": { "type": "string" },
          "riskCategory": { "type": "string" }
        }}
    },
    "reasons": {
      "type": "array",
      "items": { "type": "string" }
    },
    "inspectors": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "fullName": { "type": "string" },
          "position": { "type": "string" }
        }}
    },
    "prosecutorOrganizationName": { "type": "string" },
    "resultOfInspection": { "type": "string" },
    "reviewFlag": { "type": "boolean" },
    "reviewCompletedFlag": { "type": "boolean" },
    "refusePreventiveVisit": { "type": "boolean" },
    "appealPreventiveVisit": { "type": "boolean" },
    "isRejectSubject": { "type": "boolean" }
  }
}
